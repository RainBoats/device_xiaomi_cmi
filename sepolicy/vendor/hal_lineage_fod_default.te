type hal_lineage_fod_cas, domain;
type hal_lineage_fod_cas_exec, exec_type, vendor_file_type, file_type;

hal_server_domain(hal_lineage_fod_cas, hal_lineage_fod)

init_daemon_domain(hal_lineage_fod_cas)

wakelock_use(hal_lineage_fod_cas)

hal_client_domain(hal_lineage_fod_cas, hal_fingerprint)
hal_client_domain(hal_lineage_fod_cas, hal_touchfeature)

binder_call(hal_lineage_fod_cas, hal_fingerprint_default)
binder_call(hal_lineage_fod_cas, hal_touchfeature_default)

allow hal_lineage_fod_cas vendor_sysfs_graphics:dir search;
allow hal_lineage_fod_cas vendor_sysfs_graphics:file rw_file_perms;
